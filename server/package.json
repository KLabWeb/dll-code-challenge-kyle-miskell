{
  "name": "server",
  "version": "1.0.0",
  "description": "Kyle T Miskell DLL Code Challenge App Server",
  "author": "Kyle T Miskell kmiskell@protonmail.com",
  "scripts": {
    "dev": "nodemon ./src/app.ts",
    "start": "node ./dist/app.js",
    "build": "npx tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "kill-port": "lsof -ti:3001 | xargs kill -9 || echo 'Port 3001 is clear'",
    "kill-all": "pkill -f 'react-express-code-interview.*node' || echo 'No lingering processes found'"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.15",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.15.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "jest": "^29.5.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
